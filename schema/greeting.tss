

/** Greeting API request message */
export interface GreetingRequestMessage {
    /** My name */
    @match(/^[A-Z].+$/)
    name: string;
}


/** Greeting API response message */
export interface GreetingResponseMessage {
    /** Greeting words */
    @match(/^H.+$/)
    greeting: string;

    /** Greeting to */
    to: string;
}
